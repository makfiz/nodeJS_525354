  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${file}"
    },
    {
        "name": "Attach to Process A",
        "type": "node",
        "request": "attach",
        "skipFiles": [
          "${workspaceFolder}/node_modules/**/*",
          "${workspaceFolder}/<node_internals>/**/*",
          "<node_internals>/**",
        ],
        "port": 9229
      },
  ]
}
